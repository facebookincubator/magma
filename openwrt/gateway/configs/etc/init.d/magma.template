SERVICE_DAEMONIZE=1
SERVICE_WRITE_PID=1
SERVICE_QUIET=

if [ -z $FLGS ]; then
  FLGS="-logtostderr -stderr2syslog -stdout2stderr -v=2"
fi

start() {
  ulimit -v 30000 -m 20000
  export GODEBUG=madvdontneed=1
  service_start /sbin/$CMD $FLGS
}

stop() {
  service_stop /sbin/$CMD $FLGS
}

